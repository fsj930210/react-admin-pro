{
  "name": "@rap/hooks",
  "version": "0.0.1",
  "description": "Reusable React hooks for React Admin Pro",
  "keywords": [
    "react",
    "hooks",
    "custom-hooks",
    "react-hooks",
    "utility-hooks"
  ],
  "private": true,
  "type": "module",
  "exports": {
    "./use-mobile": {
      "types": "./src/use-mobile.ts",
      "default": "./src/use-mobile.ts"
    },
    "./use-zustand": {
      "types": "./src/use-zustand.ts",
      "default": "./src/use-zustand.ts"
    },
    "./use-move": {
      "types": "./src/use-move.ts",
      "default": "./src/use-move.ts"
    },
    "./use-resize": {
      "types": "./src/use-resize.ts",
      "default": "./src/use-resize.ts"
    },
    "./use-minMax": {
      "types": "./src/use-minMax.tsx",
      "default": "./src/use-minMax.tsx"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@rap/biome-config": "workspace:*",
    "@rap/eslint-config": "workspace:*",
    "@rap/tsconfig": "workspace:*",
    "@types/lodash-es": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "eslint-config-biome": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@rap/utils": "workspace:*",
    "immer": "catalog:",
    "lodash-es": "catalog:",
    "react": "catalog:",
    "zustand": "catalog:"
  }
}
