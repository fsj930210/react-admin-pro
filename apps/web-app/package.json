{
	"name": "@rap/web-app",
	"version": "0.0.1",
	"private": true,
	"description": "React Admin Pro SPA application built with Vite/Rsbuild, TanStack Router and React Query",
	"keywords": [
		"react",
		"vite",
		"rsbuild",
		"spa",
		"tanstack-router",
		"tanstack-query",
		"admin-dashboard",
		"tailwindcss"
	],
	"type": "module",
	"scripts": {
		"build": "rsbuild build",
		"dev": "rsbuild dev",
		"preview": "rsbuild preview --env-mode preview",
		"dev:vite": "vite",
		"build:vite": "tsc -b && vite build",
		"preview:vite": "vite preview",
		"lint:eslint": "eslint --fix",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{css}\" --cache-location  node_modules/.cache/stylelint/ --allow-empty-input",
		"lint:biome": "biome check --write",
		"lint": " pnpm lint:biome && pnpm lint:eslint && pnpm lint:stylelint"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@rap/biome-config": "workspace:*",
		"@rap/eslint-config": "workspace:*",
		"@rap/rsbuild-config": "workspace:*",
		"@rap/stylelint-config": "workspace:*",
		"@rap/tailwindcss-config": "workspace:*",
		"@rap/tsconfig": "workspace:*",
		"@rap/vite-config": "workspace:*",
		"@rsbuild/core": "catalog:",
		"@tanstack/react-router-devtools": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"eslint": "catalog:",
		"eslint-config-biome": "catalog:",
		"stylelint": "catalog:",
		"typescript": "catalog:",
		"vite": "catalog:"
	},
	"dependencies": {
		"@hookform/resolvers": "catalog:",
		"@rap/components-base": "workspace:*",
		"@rap/components-ui": "workspace:*",
		"@rap/hooks": "workspace:*",
		"@rap/styles": "workspace:*",
		"@rap/utils": "workspace:*",
		"@tanstack/react-query": "catalog:",
		"@tanstack/react-query-devtools": "catalog:",
		"@tanstack/react-router": "catalog:",
		"lucide-react": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-hook-form": "catalog:",
		"sonner": "catalog:",
		"zod": "catalog:"
	}
}
